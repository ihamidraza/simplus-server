    "ts-node": "5.0.1",
    "ts-node-dev": "1.0.0-pre.18",
    "tsify": "3.0.4",
    "tslib": "1.9.0",
    "tslint": "5.9.1",
    "typedoc": "0.11.1",
    "typescript": "2.7.1"
  }
