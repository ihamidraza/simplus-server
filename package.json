    "body-parser": "1.18.2",
    "bunyan": "1.8.12",
    "express": "^4.16.3",
    "googleapis": "^35.0.0"
  },
  "devDependencies": {
    "@types/browserify": "12.0.33",
    "@types/chai": "4.1.2",
